{
    "name": "owlookit/quickrep",
    "description": "Quickrep, a PHP reporting engine for Laravel",
    "type": "library",
    "license": "Apache-2.0",
     "keywords": [
                "laravel",
                "reporting",
                "reports",
		"report",
		"datatables",
		"d3",
		"vega"
    ],
    "require": {
        "php": ">=8.1.0",
        "doctrine/sql-formatter": "1.1.*",
        "phpoffice/phpspreadsheet": "^1.28",
        "twbs/bootstrap": "4.1.3",
      "components/jquery": "^3.6",
      "moment/moment": "^2.29",
      "fortawesome/font-awesome" : "^6.2",
      "datatables/datatables": "1.10.21"
    },
    "autoload": {
        "psr-4" : {
            "Owlookit\\Quickrep\\" : "src/"
        }
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "Owlookit\\Quickrep\\QuickrepServiceProvider"
	        ]
	    }
	},
     "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
