{
    "name": "owlookit/quickrep",
    "description": "Quickrep, a PHP reporting engine for Laravel",
    "type": "library",
    "license": "Apache-2.0",
     "keywords": [
                "laravel",
                "reporting",
                "reports"
    ],
    "require": {
        "php": ">=8.1.0",
        "doctrine/sql-formatter": "^1.2",
        "phpoffice/phpspreadsheet": "^1.29",
        "components/jquery": "^3.6",
        "moment/moment": "^2.30",
        "fortawesome/font-awesome" : "^6.5",
        "twbs/bootstrap": "4.1.3",
        "datatables/datatables": "1.10.21"
    },
    "autoload": {
        "psr-4" : {
            "Owlookit\\Quickrep\\" : "src/"
        }
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "Owlookit\\Quickrep\\QuickrepServiceProvider"
	        ]
	    }
	},
     "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
